use std
use bio

use "types"

pkg xml =
	pkglocal const mk : (f : bio.file#, owned : bool -> ctx#)
;;

const mk = {f, owned
	-> std.mk([
		.file = f,
		.owned = owned,
		.p = std.mk([
			.buf = [][:],
			.bidx = 0,
			.bcur = 0,
			.line = 1,
			.off = 0,
			.state = `Close,
			.more = true
		])
	])
}
